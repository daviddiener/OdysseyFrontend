{
  "name": "odysseyfrontend",
  "version": "0.0.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development --host 0.0.0.0 --disableHostCheck",
    "start:prod": "ng serve -c production --host 0.0.0.0 --disableHostCheck",
    "start:pwa": "ng build -c production --output-hashing none && http-server -p 4200 -c-1 dist/OdysseyFrontend",
    "build": "ng build -c production --output-hashing none && npm run purge && npm run copy",
    "build:push": "npm version patch && npm run build && cd ..\\OdysseyBackend && npm run push",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "purge": "if exist ..\\OdysseyBackend\\public\\ del /q ..\\OdysseyBackend\\public\\OdysseyFrontend\\*",
    "copy": "xcopy /R /S /Q /Y .\\dist ..\\OdysseyBackend\\public\\"
  },
  "private": true,
  "author": "David Diener",
  "license": "UNLICENSED",
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/cdk": "~11.2.6",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/localize": "~11.2.7",
    "@angular/material": "~11.2.6",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@angular/service-worker": "~11.2.7",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.0.0",
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1",
    "phaser": "^3.54.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@angular/language-service": "~11.2.7",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5"
  }
}
